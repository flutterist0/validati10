{
  "AZ": {
    "phone": {
      "regex": "^(\\+994|994|0)(50|51|55|70|77|99|10|60)\\d{7}$",
      "format_clean_regex": "[\\s\\-\\(\\)]",
      "prefix_replace": {
        "+994": "",
        "994": "+",
        "0": "+994"
      },
      "operators": {
        "50": "Azercell",
        "51": "Azercell",
        "55": "Azercell",
        "70": "Bakcell",
        "77": "Bakcell",
        "99": "Nar Mobile",
        "10": "Nakhtel",
        "60": "Ulduz Mobile"
      }
    },
    "identity": {
      "name": "FIN",
      "regex": "^[A-Z0-9]{7}$",
      "length": 7,
      "requires_digit": true,
      "requires_letter": true
    },
    "vehicle": {
      "regex": "^[0-9]{2}[A-Z]{2}[0-9]{3}$",
      "format_pattern": "##-XX-###", 
      "region_index": [0, 2]
    },
    "postal": {
      "regex": "^AZ\\d{4}(\\d{2})?$",
      "format_pattern": "XX ####"
    }
  },
  "TR": {
    "phone": {
      "regex": "^(\\+90|90|0)?5\\d{9}$",
      "format_clean_regex": "[\\s\\-\\(\\)]",
      "operators": {
        "53": "Turkcell",
        "54": "Vodafone",
        "55": "Turk Telekom"
      }
    },
    "identity": {
      "name": "TCKN",
      "regex": "^[1-9]{1}[0-9]{9}[02468]{1}$",
      "length": 11,
      "requires_digit": true,
      "requires_letter": false
    },
    "vehicle": {
      "regex": "^(0[1-9]|[1-7][0-9]|8[01])[A-Z]{1,3}[0-9]{2,4}$",
      "format_pattern": "## XX ####"
    },
    "postal": {
      "regex": "^\\d{5}$",
      "format_pattern": "#####"
    }
  }
}